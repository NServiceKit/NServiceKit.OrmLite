<?xml version="1.0"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>NServiceKit.OrmLite.MySql</id>
    <title>OrmLite.MySQL - Fast, code-first, config-free Poco ORM</title>
    <version>1.0.1</version>
    <authors>NServiceKit, Thomas Grassauer</authors> 
    <owners>NServiceKit, Thomas Grassauer</owners>
    <description>
        Light, simple and fast convention-based code-first POCO ORM for MySQL. 
		Support for Creating and Dropping Table Schemas from POCOs, Complex Property types transparently stored in schemaless text blobs in MySQL.
    </description> 
    <projectUrl>https://github.com/NServiceKit/NServiceKit.OrmLite</projectUrl>
    <licenseUrl>https://github.com/NServiceKit/NServiceKit/blob/LICENSE</licenseUrl>
    <iconUrl>http://www.nservicekit.net/logo-100x100.png</iconUrl>
    <tags>MySql OrmLite POCO Code-First Orm Schema-less Blobs</tags>
    <language>en-US</language> 
    <copyright>NServiceKit 2014 and contributors</copyright>
    <dependencies>
      <dependency id="MySql.Data" />
      <dependency id="NServiceKit.Common" version="[1,2)" />
    </dependencies> 
  </metadata> 
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\NServiceKit.OrmLite\**\*.cs" target="src\NServiceKit.OrmLite" />
    <file src="..\..\src\NServiceKit.OrmLite.MySql\**\*.cs" target="src\NServiceKit.OrmLite.MySql" />
  </files>
</package>